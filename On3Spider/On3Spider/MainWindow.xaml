<Window x:Class="On3Spider.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:On3Spider"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel Margin="10,10,10,276">
            <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top" Margin="10,0,23,10" RenderTransformOrigin="-2.025,0.485" Height="28" Width="464">
                <Button Name="btnOpenFile" Click="btnOpenFile_Click">Open file</Button>
                <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox" Width="409"/>
            </WrapPanel>
        </DockPanel>
        <DockPanel HorizontalAlignment="Left" Height="31" LastChildFill="False" Margin="20,48,0,0" VerticalAlignment="Top" Width="471">
            <ProgressBar Height="31" VerticalAlignment="Top" Width="471"/>
        </DockPanel>
        <DockPanel HorizontalAlignment="Left" Height="225" LastChildFill="False" Margin="20,84,0,0" VerticalAlignment="Top" Width="471"/>
    </Grid>
</Window>
